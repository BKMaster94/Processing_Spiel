package Game;

import controlP5.*;
import processing.core.PApplet;

public class gameButtons {

	haupt hauptforbutton = new haupt();
	ControlP5 buttonForAll = new ControlP5(hauptforbutton);
	
	public void buttonsetup(){
	buttonForAll.addButton("test")
	.setValue(0)
	.setPosition(50,700)
	.setSize(100,50);
	}
	
	
	
	
}
