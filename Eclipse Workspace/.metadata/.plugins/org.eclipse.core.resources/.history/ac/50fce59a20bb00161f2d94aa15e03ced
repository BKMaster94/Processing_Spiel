package Game;

public class Leben {

	public int lebenChar = 100;
	public int bossLeben = 200;
	
	
	int CharLebenVerlust(int verlust){
		lebenChar= lebenChar - verlust;
		System.out.println("Char Leben: " + lebenChar);
		return lebenChar;
	}

	int BossLebenVerlust(int verlust){
		bossLeben = bossLeben - verlust;
		System.out.print("Boss Leben: " + bossLeben);
		return bossLeben;
	}
	
	void CharLeben(){
		
		
	}
	
	void LebensAnzeige(){
		
		
	}
	
	
	
}
