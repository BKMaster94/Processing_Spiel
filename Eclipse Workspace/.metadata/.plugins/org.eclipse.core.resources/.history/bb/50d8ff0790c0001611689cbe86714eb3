package Game;

import controlP5.*;
import processing.core.PApplet;
public class gameButtons {

	haupt hauptforbutton;
	ControlP5 buttonForAll;
	
	public void buttonsetup(){
		hauptforbutton = new haupt();
		buttonForAll = new ControlP5(hauptforbutton);
	buttonForAll.addButton("test")
	.setValue(0)
	.setPosition(200,700)
	.setSize(100,50);
	}
	
	
	
	
}
