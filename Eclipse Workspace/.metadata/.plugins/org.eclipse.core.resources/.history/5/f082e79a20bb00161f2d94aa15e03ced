package Game;



public class hitCollisionUmgebung {
	//////////// Initialisierung
	BackagroundLoader test = new BackagroundLoader();
	
	
	
	
	////////////
	
	
	public boolean collisionright(boolean darflaufen, int bewegungseitlich,int bewegunghorizontal){
		
		if (test.backgroundid == 0){
			if(bewegungseitlich >= 20 && bewegungseitlich <= 50 && bewegunghorizontal >=20 && bewegunghorizontal <=50 ){
				darflaufen = false;
		}else{
			darflaufen = true;
		}
		
		
		
		
		
	}
		return darflaufen;
	}
}
